{
  "name": "perez.io",
  "version": "3.0.2",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.3.3",
    "i18next": "^23.12.2",
    "i18next-xhr-backend": "^3.2.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-magic-motion": "^1.1.2",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "sass": "^1.77.8",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "new-start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "mac-start": "export NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-git": "set PUBLIC_URL=https://nperez9.github.io/nperez.io&& npm run build",
    "build-git-linux": "export PUBLIC_URL=https://nperez9.github.io/nperez.io&& npm run build",
    "build-prod": "set PUBLIC_URL=/&& npm run build",
    "pre-deploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "gh-pages": "^6.1.1"
  }
}
